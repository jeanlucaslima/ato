{
  "name": "ato",
  "version": "3.0.0",
  "description": "Advanced Tab Organizer - Chrome extension with superpowers for managing tabs",
  "main": "dist/manifest.json",
  "scripts": {
    "test": "vitest",
    "dev": "vite build --watch --mode development",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "clean": "rimraf dist",
    "zip": "npm run build && cd dist && zip -r ../ato-extension.zip ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanlucaslima/ato.git"
  },
  "keywords": "[\"chrome-extension\", \"tab-management\", \"productivity\", \"browser-extension\", \"react\", \"vite\"]",
  "author": "Jean Lucas Lima <jeanleonino@gmail.com>",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/jeanlucaslima/ato/issues"
  },
  "homepage": "https://github.com/jeanlucaslima/ato#readme",
  "dependencies": {
    "fuse.js": "^7.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.326",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": "[\"last 1 chrome version\"]",
    "development": "[\"last 1 chrome version\"]"
  }
}
