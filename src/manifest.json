{
  "author": "Jean Lucas Lima <github.com/jeanlucaslima>",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "64": "img/icon_64.png"
    },
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  },
  "description": "A search based Chrome extension to organize tabs, windows, and groups.",
  "icons": {
    "16": "img/icon_16.png",
    "32": "img/icon_32.png",
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "manifest_version": 3,
  "name": "Advanced Tab Manager",

  "permissions": [
    "tabs"
  ],
  "short_name": "ato",
  "version": "0.1"
}
